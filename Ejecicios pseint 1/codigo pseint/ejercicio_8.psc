Algoritmo ejercicio_8
	Escribir 'Ingrese el numero 1'
	Leer numero1
	Escribir 'Ingrese el numero 2'
	Leer numero2
	residuo_division <- numero1 MOD numero2
	division_entera <-  (numero1 - residuo_division)/numero2
	Escribir 'El resultado de la división es: ', division_entera, ' que tiene como residuo/modulo: ', residuo_division
FinAlgoritmo
