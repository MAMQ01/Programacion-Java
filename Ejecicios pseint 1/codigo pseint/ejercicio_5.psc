Algoritmo Ejercicio_5
	Escribir "calcular la circunferencia, el area y el volumen"
	Escribir "Escriba el radio: "
	Leer radio
	longitud_circunferencia <- radio*2*pi
	Escribir "la longitud de la circunferencia es: ", longitud_circunferencia
	area <- pi *radio*radio
	Escribir "El area es: ", area
	volumen <- 4/3 * pi * (radio*radio*radio)
	Escribir "El volumen es: ", volumen
FinAlgoritmo
